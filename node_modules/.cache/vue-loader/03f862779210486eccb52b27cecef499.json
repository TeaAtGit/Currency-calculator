\n  props: [\"number\"],\r\n  data() {\r\n    return {\r\n      storedData: [],\r\n      inputValue: \"\",\r\n      priceInHRK: 1,\r\n      // priceInEUR: this.currencyRounded,\r\n      currencyEUR: \" EUR = \",\r\n      currencyHRK: \" HRK \",\r\n    };\r\n  },\r\n  created() {\r\n    fetch(\"http://localhost:3000/podaciHNB/\")\r\n      .then((res) => res.json())\r\n      .then((data) => (this.storedData = data[0]))\r\n      .catch((err) => console.log(err.message));\r\n  },\r\n\r\n  computed: {\r\n    currencyRounded: function() {\r\n      return this.formatCurrency(this.storedData[\"Srednji za devize\"]);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    formatCurrency(value) {\r\n      const replacedCommasWithDots = value.replace(/,/g, \".\");\r\n      const stringToNumber = parseFloat(replacedCommasWithDots);\r\n      const roundNum = stringToNumber.toFixed(2);\r\n      return roundNum;\r\n    },\r\n\r\n    changeCurrency() {\r\n      const srednjiZaDevize = this.storedData[\"Srednji za devize\"];\r\n      const result = this.formatCurrency(srednjiZaDevize);\r\n      // console.log(typeof rezultat);\r\n      // console.log(this.inputValue / rezultat);\r\n      // // return this.inputValue / rezultat;\r\n      this.priceInHRK = this.inputValue;\r\n      this.currencyEUR = \" HRK = \";\r\n      this.currencyRounded = (this.inputValue / result).toFixed(2);\r\n\r\n      this.currencyHRK = \" EUR\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#title {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: left;\r\n  color: black;\r\n  font-size: 10px;\r\n  margin-top: 30px;\r\n  margin-left: 30px;\r\n}\r\n\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  margin-top: 30px;\r\n}\r\n\r\n#input {\r\n  height: 40px;\r\n  min-width: 100px;