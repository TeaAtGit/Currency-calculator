r />\r\n  <Button title=\"pretvori\" @click=\"pretvori(inputValue)\" />\r\n</template>\r\n\r\n<script>\r\nimport Button from \"./Button\";\r\nexport default {\r\n  name: \"App\",\r\n  components: {\r\n    Button,\r\n  },\r\n  props: [\"number\"],\r\n  data() {\r\n    return {\r\n      podaci: [],\r\n      inputValue: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    fetch(\"http://localhost:3000/podaciHNB/\")\r\n      .then((res) => res.json())\r\n      .then((data) => (this.podaci = data[0]))\r\n      .catch((err) => console.log(err.message));\r\n  },\r\n  methods: {\r\n    formatirajPodatak(value) {\r\n      let rplCommWDots = value.replace(/,/g, \".\");\r\n      let strToNum = parseFloat(rplCommWDots);\r\n      let roundNum = strToNum.toFixed(2);\r\n      return roundNum;\r\n    },\r\n\r\n    pretvori() {\r\n      let srednjiZaDevize = this.podaci[0][\"Srednji za devize\"];\r\n      let rezultat = this.formatirajPodatak(srednjiZaDevize);\r\n      // console.log(typeof rezultat);\r\n      // console.log(this.inputValue / rezultat);\r\n      // // return this.inputValue / rezultat;\r\n      document.getElementById(\"numberEuro\").innerHTML = (\r\n        this.inputValue / rezultat\r\n      ).toFixed(2);\r\n      document.getElementById(\"numberHRK\").innerHTML = this.inputValue;\r\n      document.getElementById(\"changeToHRK\").innerHTML = \" HRK = \";\r\n      document.getElementById(\"changeToEuro\").innerHTML = \" EUR\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#title {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: left;\r\n  color: black;\r\n  font-size: 10px;\r\n  margin-top: 30px;\r\n  margin-left: 30px;\r\n}\r\n\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  margin-top: 30px;\r\n}\r\n\r\n#input {\r\n  