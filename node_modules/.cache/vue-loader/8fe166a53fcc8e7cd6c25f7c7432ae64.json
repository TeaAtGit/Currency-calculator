EAAE,CAAC,CAAC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACZ,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AACd","file":"C:/Users/amare/Documents/Javascript/HNB_projekt/src/components/Input.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div>\r\n    <h1>\r\n      <span id=\"numberHRK\">1</span>\r\n      <span id=\"changeToHRK\"> EUR = </span>\r\n      <span id=\"numberEuro\">\r\n        {{ formatirajPodatak(this.podaci[\"Srednji za devize\"]) }}\r\n      </span>\r\n      <span id=\"changeToEuro\"> HRK</span>\r\n    </h1>\r\n    <h3>Vrijedi za: {{ this.podaci[\"Datum primjene\"] }}</h3>\r\n    <input\r\n      type=\"number\"\r\n      v-model.number=\"inputValue\"\r\n      placeholder=\"UpiÅ¡i iznos u kunama...\"\r\n      @keyup.enter=\"pretvori(inputValue)\"\r\n    />\r\n  </div>\r\n  <br />\r\n  <Button title=\"pretvori\" @click=\"pretvori(inputValue)\" />\r\n</template>\r\n\r\n<script>\r\nimport Button from \"./Button\";\r\nexport default {\r\n  name: \"Input\",\r\n  components: {\r\n    Button,\r\n  },\r\n  props: [\"number\"],\r\n  data() {\r\n    return {\r\n      podaci: [],\r\n      inputValue: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    fetch(\"http://localhost:3000/podaciHNB/\")\r\n      .then((res) => res.json())\r\n      .then((data) => (this.podaci = data[0]))\r\n      .catch((err) => console.log(err.message));\r\n  },\r\n  methods: {\r\n    formatirajPodatak(value) {\r\n      let rplCommWDots = value.replace(/,/g, \".\");\r\n      let strToNum = parseFloat(rplCommWDots);\r\n      let roundNum = strToNum.toFixed(2);\r\n      return roundNum;\r\n    },\r\n\r\n    pretvori() {\r\n      let srednjiZaDevize = this.podaci[0][\"Srednji za devize\"];\r\n      let rezultat = this.formatirajPodatak(srednjiZaDevize);\r\n      // console.log(typeof rezultat);\r\n      // console.log(this.inputValue / rezultat);\r\n      // // return this.inputValue / rezultat;\r\n      document.getElementById(\"numberEuro\").innerHTML = (\r\n        this.inputValue / rezultat\r\n      ).toFixed(2);\r\n      document.getElementById(\"numberHRK\").innerHTML = this.inputValue;\r\n      document.getElementById(\"changeToHRK\").innerHTML = \" HRK = \";\r\n      document.getElementById(\"changeToEuro\").innerHTML = \" EUR\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput {\r\n  height: 40px;\r\n  min-width: 100px;\r\n  background-color: rgb(58, 56, 56);\r\n  color: white;\r\n  border: 0;\r\n  border-radius: 4px;\r\n  margin: 20px;\r\n  width: 300px;\r\n}\r\n</style>\r\n"]}]}