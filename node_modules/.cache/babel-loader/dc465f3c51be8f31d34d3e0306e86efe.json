i[0][\"Srednji za devize\"];\r\n      let rezultat = this.formatirajPodatak(srednjiZaDevize);\r\n      // console.log(typeof rezultat);\r\n      // console.log(this.inputValue / rezultat);\r\n      // // return this.inputValue / rezultat;\r\n      document.getElementById(\"numberEuro\").innerHTML = (\r\n        this.inputValue / rezultat\r\n      ).toFixed(2);\r\n      document.getElementById(\"numberHRK\").innerHTML = this.inputValue;\r\n      document.getElementById(\"changeToHRK\").innerHTML = \" HRK = \";\r\n      document.getElementById(\"changeToEuro\").innerHTML = \" EUR\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput {\r\n  height: 40px;\r\n  min-width: 100px;\r\n  background-color: rgb(58, 56, 56);\r\n  color: white;\r\n  border: 0;\r\n  border-radius: 4px;\r\n  margin: 20px;\r\n  width: 300px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}