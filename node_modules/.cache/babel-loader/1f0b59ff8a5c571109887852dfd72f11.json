\n    formatCurrency(value) {\r\n      const replacedCommasWithDots = value.replace(/,/g, \".\");\r\n      const stringToNumber = parseFloat(replacedCommasWithDots);\r\n      const roundNum = stringToNumber.toFixed(2);\r\n      return roundNum;\r\n    },\r\n\r\n    changeCurrency() {\r\n      const srednjiZaDevize = this.storedData[\"Srednji za devize\"];\r\n      const result = this.formatCurrency(srednjiZaDevize);\r\n      // console.log(typeof rezultat);\r\n      // console.log(this.inputValue / rezultat);\r\n      // // return this.inputValue / rezultat;\r\n      this.priceInHRK = this.inputValue;\r\n      this.currencyEUR = \" HRK = \";\r\n      this.currencyRounded = (this.inputValue / result).toFixed(2);\r\n\r\n      this.currencyHRK = \" EUR\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#title {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: left;\r\n  color: black;\r\n  font-size: 10px;\r\n  margin-top: 30px;\r\n  margin-left: 30px;\r\n}\r\n\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  margin-top: 30px;\r\n}\r\n\r\n#input {\r\n  height: 40px;\r\n  min-width: 100px;\r\n  background-color: rgb(58, 56, 56);\r\n  color: white;\r\n  border: 0;\r\n  border-radius: 4px;\r\n  margin: 20px;\r\n  width: 300px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}